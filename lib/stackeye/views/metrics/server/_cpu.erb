<div class="card">
  <div class="card-header">
    <div class="row align-items-center">
      <div class="col">
        <h4 class="card-header-title">
          CPU
        </h4>
      </div>
      <div class="col-auto">
        <ul class="nav nav-tabs nav-tabs-sm card-header-tabs">
          <li class="nav-item" data-toggle="chart" data-type="time" data-target="cpuChart" data-metrics="<%= @metrics.pluck('cpu_load_1m') %>" data-mean="<%= @metrics.mean('cpu_load_1m').round(1) %>" data-timestamps="<%= @metrics.pluck('timestamp') %>">
            <a class="nav-link active" href="#" data-toggle="tab">
              1m loadavg
            </a>
          </li>
          <li class="nav-item" data-toggle="chart" data-type="time" data-target="cpuChart" data-metrics="<%= @metrics.pluck('cpu_utilization') %>" data-mean="<%= @metrics.mean('cpu_utilization').round(2) %>" data-timestamps="<%= @metrics.pluck('timestamp') %>">
            <a class="nav-link" href="#" data-toggle="tab">
              Utilization
            </a>
          </li>
        </ul>
      </div>
    </div>
  </div>
  <div class="card-body">
    <div class="chart">
      <canvas id="cpuChart" class="chart-canvas chartjs-render-monitor" width="1816" height="600" style="display: block; height: 300px; width: 908px;"></canvas>
    </div>
  </div>
  <div class="card-footer">
    Mode Avg Range
  </div>
</div>



<div class="row">
  <div class="col-12 col-lg-6 col-xl">
    <div class="card">
      <div class="card-body">
        <div class="row align-items-center">
          <div class="col">
            <h6 class="card-title text-uppercase text-muted mb-2">
              CPU 1m mean
            </h6>
            <span class="h2 mb-0">
              $24,500
            </span>
            <span class="badge badge-soft-success mt--1">
              +3.5%
            </span>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-12 col-lg-6 col-xl">
    <div class="card">
      <div class="card-body">
        <div class="row align-items-center">
          <div class="col">
            <h6 class="card-title text-uppercase text-muted mb-2">
              CPU 1m Median
            </h6>
            <span class="h2 mb-0">
              763.5
            </span>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-12 col-lg-6 col-xl">
    <div class="card">
      <div class="card-body">
        <div class="row align-items-center">
          <div class="col">
            <h6 class="card-title text-uppercase text-muted mb-2">
              CPU 1m Mode
            </h6>
            <div class="row align-items-center no-gutters">
              <div class="col-auto">
                <span class="h2 mr-2 mb-0">
                  84.5%
                </span>
              </div>
              <div class="col">
                <div class="progress progress-sm">
                  <div class="progress-bar" role="progressbar" style="width: 85%" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-12 col-lg-6 col-xl">
    <div class="card">
      <div class="card-body">
        <div class="row align-items-center">
          <div class="col">
            <h6 class="card-title text-uppercase text-muted mb-2">
              CPU 1m 95%
            </h6>
            <span class="h2 mb-0">
              $85.50
            </span>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
